        <td>
            @Html.DisplayFor(modelItem => item.CLIENT.US)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRODUCT.NAMEPD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SELLNUMBER)
        </td>
